{
  "name": "coding-boilerplate",
  "version": "1.8.2",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all -p dev:*",
    "build": "cross-env NODE_ENV=production npm-run-all -s dev:*",
    "dev:server": "node config/scripts/server.js",
    "dev:copy": "node config/scripts/copy.js",
    "dev:ejs": "node config/scripts/ejs.js",
    "dev:sass": "node config/scripts/sass.js",
    "dev:babel": "node config/scripts/babel.js",
    "lint:html": "node config/scripts/lint_html.js",
    "format:prettier": "node config/scripts/prettier.js",
    "tools": "node config/tools/tools.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm-run-all -p lint:*"
    }
  },
  "devDependencies": {
    "autoprefixer": "9.4.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "browser-sync": "2.26.3",
    "cross-env": "5.2.0",
    "css-mqpacker": "7.0.0",
    "csv-parse": "4.0.1",
    "ejs": "2.6.1",
    "fs-extra": "7.0.1",
    "fuse-box": "3.7.0-next.9",
    "globby": "8.0.1",
    "html5-validator": "1.2.1",
    "htmlhint": "0.10.1",
    "husky": "1.2.0",
    "imagemin": "6.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "6.0.0",
    "imagemin-svgo": "7.0.0",
    "node-notifier": "5.3.0",
    "node-sass": "4.10.0",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.6",
    "prettier": "1.15.3",
    "puppeteer-core": "1.11.0",
    "ssi": "0.3.0",
    "typescript": "3.2.1",
    "uglify-es": "3.3.9",
    "uglify-js": "3.4.9"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  }
}
