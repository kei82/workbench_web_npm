{
  "name": "coding-boilerplate",
  "version": "1.9.2",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all server",
    "build": "cross-env NODE_ENV=production npm-run-all -s build:*",
    "server": "webpack-dev-server",
    "build:clean": "webpack",
    "build:webpack": "webpack",
    "build:copy": "cpx 'src/**/[!sass,bable]*/*' 'dist'",
    "build:ejs": "node config/scripts/ejs.js",
    "tools": "node config/tools/tools.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": ""
    }
  },
  "lint-staged": {
    "*.html": [
      "htmlhint"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "autoprefixer": "9.4.2",
    "babel-loader": "8.0.4",
    "cpx": "^1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "2.0.0",
    "css-mqpacker": "7.0.0",
    "ejs": "2.6.1",
    "eslint": "5.10.0",
    "fs-extra": "7.0.1",
    "globby": "8.0.1",
    "htmlhint": "0.10.1",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.6",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "sass-loader": "7.1.0",
    "ssi": "0.3.0",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "optionalDependencies": {
    "puppeteer-core": "1.11.0",
    "imagemin": "6.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "6.0.0",
    "imagemin-svgo": "7.0.0",
    "html-validator": "3.1.3",
    "csv-parse": "4.1.0"
  },
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  }
}
